{
  "name": "ur5",
  "version": "0.47.0-beta",
  "description": "Platform for manage UniversalRobotics UR5",
  "main": "out/main.js",
  "author": {
    "email": "ls-micro@ya.ru",
    "name": "Yuuki Wesp"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "universalrobotics",
    "ur5-robot",
    "ur5-platform",
    "typescript",
    "polyscope",
    "robot",
    "ur5",
    "robot-modes"
  ],
  "bugs": {
    "url": "https://github.com/0xF6/UniversalRobotics/issues"
  },
  "homepage": "https://github.com/0xF6/UniversalRobotics#readme",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-store": "^2.0.0",
    "electron-updater": "^2.21.10",
    "electron-window-state-manager": "^0.3.2",
    "glamor": "^2.20.40",
    "jquery": "^3.3.1",
    "linqable.ts": "^1.7.2",
    "mathjs": "^4.2.2",
    "mousetrap": "^1.6.2",
    "node-file-exists": "^1.1.0",
    "node-int64": "^0.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "sleep-promise": "^6.0.0"
  },
  "scripts": {
    "compile": "NODE_ENV=production ts-node fuse",
    "start": "ts-node fuse",
    "dist": "electron-builder --linux --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xF6/UniversalRobotics.git"
  },
  "build": {
    "appId": "com.universalrobot.ur5",
    "files": [
      "!src",
      "!tsconfig.json",
      "!README.md",
      "!LICENSE",
      "!fuse.js"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.rijndael.ur5"
    },
    "publish": {
      "owner": "0xF6",
      "provider": "github",
      "repo": "UniversalRobotics"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    }
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/fuse": "^2.6.0",
    "@types/jquery": "^3.3.2",
    "@types/mathjs": "^3.20.0",
    "@types/node": "^10.1.2",
    "@types/node-int64": "^0.4.29",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-test-renderer": "^16.0.0",
    "ava": "^1.0.0-beta.5.1",
    "electron": "2.0.1",
    "electron-builder": "^20.15.1",
    "fuse": "^0.4.0",
    "fuse-box": "^3.2.2",
    "husky": "^0.14.3",
    "react-powerplug": "^1.0.0-alpha.7",
    "react-test-renderer": "^16.4.0",
    "ts-node": "^6.1.0",
    "typescript": "^2.8.3"
  }
}
